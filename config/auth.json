{
  "serviceName": "Cognito",
  "useDefault": "manual",
  "resourceName": "userPool",
  "defaultAuthentication": {
    "authenticationType": "AMAZON_COGNITO_USER_POOLS"
  },
  "additionalAuthenticationProviders": [],
  "mfaConfiguration": "OFF",
  "passwordPolicy": {
    "minLength": 8,
    "additionalConstraints": [
      "REQUIRE_LOWERCASE",
      "REQUIRE_UPPERCASE",
      "REQUIRE_NUMERIC"
    ]
  },
  "usernameAttributes": [
    "EMAIL"
  ],
  "requiredAttributes": [
    "EMAIL"
  ],
  "userPoolGroups": false,
  "adminQueries": false
}